{% extends 'templates/default.twig' %}
{% block content %}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="page-header">
                <h1><strong>405</strong> | That's an error.</h1>
            </div>
            <h4>Your requested method (<strong>{{ method }}</strong>) is not allowed for <strong>{{ request_uri }}</strong> <br> <small>Acceptable Methods: <strong>{{ methods }}</strong></small></h4>
        </div>
    </div>
</div>
{% endblock %}
